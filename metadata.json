{
    "name": "openstack-tempest",
    "version": "9.6.0",
    "author": "OpenStack Contributors",
    "summary": "Puppet module for OpenStack Tempest",
    "license": "Apache-2.0",
    "source": "git://github.com/openstack/puppet-tempest.git",
    "project_page": "https://launchpad.net/puppet-tempest",
    "issues_url": "https://bugs.launchpad.net/puppet-tempest",
    "requirements": [
        {
            "name": "pe",
            "version_requirement": "3.x"
        },
        {
            "name": "puppet",
            "version_requirement": "3.x"
        }
    ],
    "operatingsystem_support": [
        {
            "operatingsystem": "Debian",
            "operatingsystemrelease": [
                "7"
            ]
        },
        {
            "operatingsystem": "Fedora",
            "operatingsystemrelease": [
                "20"
            ]
        },
        {
            "operatingsystem": "RedHat",
            "operatingsystemrelease": [
                "6.5",
                "7"
            ]
        },
        {
            "operatingsystem": "Ubuntu",
            "operatingsystemrelease": [
                "14.04",
                "16.04"
            ]
        }
    ],
    "description": "Installs and configures OpenStack Tempest (Quality Assurance).",
    "dependencies": [
        {
            "name": "puppetlabs/inifile",
            "version_requirement": ">=1.0.0 <2.0.0"
        },
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": ">=4.0.0 <5.0.0"
        },
        {
            "name": "puppetlabs/vcsrepo",
            "version_requirement": ">=0.1.2 <2.0.0"
        },
        {
            "name": "openstack/openstacklib",
            "version_requirement": ">=9.6.0 <10.0.0"
        },
        {
            "name": "openstack/oslo",
            "version_requirement": ">=9.6.0 <10.0.0"
        }
    ]
}